<!DOCTYPE html>
<html ng-app="myApp">
<head>
<script src="http://cdnjs.cloudflare.com/ajax/libs/angular.js/1.2.1/angular.min.js"></script>
<link href="http://cdn.kendostatic.com/2013.3.1119/styles/kendo.common.min.css" rel="stylesheet" type="text/css" />
<link href="http://cdn.kendostatic.com/2013.3.1119/styles/kendo.default.min.css" rel="stylesheet" type="text/css" />
<script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
<script src="http://cdn.kendostatic.com/2013.3.1119/js/kendo.all.min.js"></script>
<script src="http://jsbin.com/gimagiko/1.js"></script>
  <meta charset="utf-8">
  <title></title>
</head>
<body ng-controller="HomeCtrl">

  <input ng-model="name" class="k-textbox">
  <input kendo-auto-complete ng-model="country" k-data-source="countries">
  <button class="k-button" ng-click="add()">Add</button>
  <hr>
  
  <h3>Kendo UI Grid</h3>
  <div kendo-grid k-data-source="people" k-columns="[{ title: 'Name', field: 'name' }, { title: 'Country', field: 'country' }]"></div>
  
  <h3>Standard ng-repeat</h3>
  <ul>
    <li ng-repeat="person in people">{{ person.name }} - {{ person.country }}</li>
  </ul>
  
  <script>
  	
    angular.module('myApp', ['kendo.directives']);

    function HomeCtrl($scope) {

      $scope.countries = [ "Albania", "Andorra", "Armenia", "Austria", "Azerbaijan", "Belarus", "Belgium", "Bosnia & Herzegovina", "Bulgaria", "Canada", "Croatia", "Cyprus", "Czech Republic", "Denmark", "Estonia", "Finland", "France", "Georgia", "Germany", "Greece", "Hungary", "Iceland", "Ireland", "Italy", "Kosovo", "Latvia", "Liechtenstein", "Lithuania", "Luxembourg", "Macedonia", "Malta", "Moldova", "Monaco", "Montenegro", "Netherlands", "Norway", "Poland", "Portugal", "Romania", "Russia", "San Marino", "Serbia", "Slovakia", "Slovenia", "Spain", "Sweden", "Switzerland", "Turkey", "Ukraine", "United Kingdom", "United States", "Vatican City" ];

      $scope.people = new kendo.data.ObservableArray([
        { name: "Mihai Bazon", country: "Romania" },
        { name: "Burke Holland", country: "USA" },
        { name: "Pierre Asselin", country: "Canada" }
      ]);

      $scope.add = function () {
        $scope.people.push({ name: $scope.name, country: $scope.country });
      };

    }
    
  </script>
  
</body>
</html>